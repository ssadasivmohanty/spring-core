<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean name="emp1" class="com.springcore.collections.Emp">
        <property name="name"  value="Sourav"/>
        <property name="phones">
            <list>
                <value>7894561230</value>
                <value>75125</value>
                <value>78652</value>
                <value>632658</value>

            </list>
        </property>
        <property name="address">
            <set>
                <value>Anduli</value>
                <value>Kdp</value>
                <value>BBSR</value>
                <value>ctc</value>

            </set>
        </property>
        <property name="courses">
            <map>
                <entry key="JAVA" value="2month"/>
                <entry key="Python" value="2month"/>
                <entry key="C" value="1month"/>

            </map>
        </property>
        <property name="details">
            <props>
                <prop key="name">Sourabha</prop>
                <prop key="address">Anduli</prop>
            </props>
        </property>

    </bean>
</beans>